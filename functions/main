def multiplicarUltimo(a):
  max=len(a)
  return (a[max-1]*3)
def nuevaListaPolos(a):
  inicio=a[0]
  fin=a[len(a)-1]
  b=[inicio,fin]
  return b
def cargarLista(a):
  data=int(input("ingrese un nro: "))
  while data!=0:
    a.append(data)
    data=int(input("ingrese un nro: "))
  return a
edades=[]
edades=cargarLista(edades)
cargarLista
i=0
while i<(len(edades)):			
  if edades[i]<30:
	  print("bye")
  else:
  	print("hi")
  i+=1
print(multiplicarUltimo(edades))
print(nuevaListaPolos(edades))